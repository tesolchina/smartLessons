<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.tesolchina.dailyassistant.pdfcleaner</string>

  <key>ProgramArguments</key>
  <array>
    <string>/bin/zsh</string>
    <string>/Users/simonwang/Documents/Usage/VibeCoding/DailyAssistant/operating/pdf_md_cleaner/scheduler/run_daily.sh</string>
  </array>

  <key>WorkingDirectory</key>
  <string>/Users/simonwang/Documents/Usage/VibeCoding/DailyAssistant</string>

  <key>StartCalendarInterval</key>
  <dict>
    <key>Hour</key>
    <integer>12</integer>
    <key>Minute</key>
    <integer>30</integer>
  </dict>

  <key>StandardOutPath</key>
  <string>/Users/simonwang/Documents/Usage/VibeCoding/DailyAssistant/operating/pdf_md_cleaner/scheduler/last_run.out</string>

  <key>StandardErrorPath</key>
  <string>/Users/simonwang/Documents/Usage/VibeCoding/DailyAssistant/operating/pdf_md_cleaner/scheduler/last_run.err</string>

  <key>RunAtLoad</key>
  <false/>
  <key>AbandonProcessGroup</key>
  <true/>
  <key>KeepAlive</key>
  <false/>
  <key>Nice</key>
  <integer>1</integer>

  <!-- Do not wake the Mac to run -->
  <key>StartOnMount</key>
  <false/>
</dict>
</plist>
